chembl:hasQUDT api:name "activity_unit" ;
        api:label "activity_unit" ;
        api:value "The unit in which {activity_value} is given. See /pharmacology/filters/units for allowed values. For e.g. IC50: /pharmacology/filters/units/IC50" ;
        api:filterVariable "?type";
        a rdf:Property .

<#pharmacologyFiltersTypesCountViewer> a api:Viewer ;
	api:name "pharmacologyFiltersTypesCountViewer" ;
	api:template
"<http://www.ebi.ac.uk/chembl> ops:activity_type_count ?count " ;
	api:where
"{
SELECT DISTINCT ?type_label COUNT(?type) AS ?count WHERE {
GRAPH <http://www.ebi.ac.uk/chembl> {
        ?type a chembl:Activity .
}
} }" .
