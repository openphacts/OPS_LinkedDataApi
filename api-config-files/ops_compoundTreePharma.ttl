	api:name "Compound Class Pharmacology Paginated" ;
	api:description "A page of items corresponding to acitivity values for a given Compound class. Currently only the ChEBI Ontology is supported." ;
	api:resultsDescription "?item <span style=\"BACKGROUND-COLOR: #66FFCC\">chembl:hasMolecule ?compound_chembl</span> ;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\">chembl:standardType ?activity_type</span> ;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\">chembl:standardRelation ?activity_relation</span> ;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\">chembl:standardValue ?activity_value</span> ;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\">chembl:standardUnits ?activity_unit</span> ;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\"> chembl:pChembl ?pChembl </span>;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\">chembl:publishedType ?published_type</span> ;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\">chembl:publishedRelation ?published_relation</span> ;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\">chembl:publishedValue ?published_value</span> ;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\">chembl:publishedUnits ?published_unit</span> ;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\">chembl:hasQUDT ?qudt_uri</span> ;
	<span style=\"BACKGROUND-COLOR: #66FFCC\">chembl:hasAssay ?assay_uri</span> ;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\">chembl:pChembl ?pChembl</span> ;
	<span style=\"BACKGROUND-COLOR: #FFCC99\">chembl:hasDocument ?doi</span> ;
	<span style=\"BACKGROUND-COLOR: #FFCC99\">bibo:pmid ?pmid</span> ;
	<span style=\"BACKGROUND-COLOR: #66FFCC\">void:inDataset &lt;http://www.ebi.ac.uk/chembl&gt;</span> .
?assay_uri <span style=\"BACKGROUND-COLOR: #A9A9F5\">chembl:hasTarget ?target_uri</span> ;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\">chembl:assayOrganismName ?assay_organism</span> ;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\">dcterms:description ?assay_description</span> ;
	<span style=\"BACKGROUND-COLOR: #66FFCC\">void:inDataset &lt;http://www.ebi.ac.uk/chembl&gt;</span> .
?target_uri <span style=\"BACKGROUND-COLOR: #66FFCC\">dcterms:title ?target_name</span> ;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\">chembl:targetOrganismName ?target_organism</span> ;
	<span style=\"BACKGROUND-COLOR: #66FFCC\">void:inDataset &lt;http://www.ebi.ac.uk/chembl&gt;</span> .
?compound_chembl <span style=\"BACKGROUND-COLOR: #66FFCC\">skos:exactMatch ?compound_cw</span> ;
	<span style=\"BACKGROUND-COLOR: #66FFCC\">skos:exactMatch ?compound_ocrs</span> ;
	<span style=\"BACKGROUND-COLOR: #A9A9F5\">skos:exactMatch ?class</span> ;
	<span style=\"BACKGROUND-COLOR: #66FFCC\">void:inDataset &lt;http://www.ebi.ac.uk/chembl&gt;</span> .
?compound_cw <span style=\"BACKGROUND-COLOR: #66FFCC\">skos:prefLabel ?compound_name</span> ;
	<span style=\"BACKGROUND-COLOR: #66FFCC\">void:inDataset &lt;http://www.conceptwiki.org&gt;</span> .
?compound_ocrs <span style=\"BACKGROUND-COLOR: #66FFCC\">ops:smiles ?smiles</span> ;
	<span style=\"BACKGROUND-COLOR: #66FFCC\">ops:inchi ?inchi</span> ;
	<span style=\"BACKGROUND-COLOR: #66FFCC\">ops:inchikey ?inchiKey</span> ;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\"> ops:molweight ?molweight </span> ;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\">ops:ro5_violations ?num_ro5_violations</span> ;
	<span style=\"BACKGROUND-COLOR: #66FFCC\">void:inDataset &lt;http://ops.rsc.org&gt;</span> .
" ;
	api:exampleRequestPath "/compound/tree/pharmacology/pages?uri=http%3A%2F%2Fpurl.obolibrary.org%2Fobo%2FCHEBI_24431";
	api:exampleRequestPath "/compound/tree/pharmacology/pages?uri=http%3A%2F%2Fpurl.obolibrary.org%2Fobo%2FCHEBI_24431&assay_organism=Mus%20musculus&minEx-activity_value=10000&activity_unit=nanomolar";
	api:selector [
		api:where 
"GRAPH <http://www.ebi.ac.uk/chebi/inference>  {
	?class rdfs:subClassOf ?ops_item
}
GRAPH <http://www.ebi.ac.uk/chebi> {
	?chebi_compound rdfs:subClassOf|rdfs:subClassOf/owl:someValuesFrom ?class .
}
GRAPH <http://ops.rsc.org> {
	?compound_ocrs skos:exactMatch ?compound_chembl .
	?compound_ocrs cheminf:CHEMINF_000018 ?smiles ;
		cheminf:CHEMINF_000396 ?inchi ;
		cheminf:CHEMINF_000399 ?inchiKey .
}
GRAPH <http://www.conceptwiki.org> {
	?compound_cw skos:exactMatch ?compound_ocrs ;
		skos:prefLabel ?compound_name .
}
GRAPH <http://www.ebi.ac.uk/chembl> {
	?item a chembl:Activity ;
		chembl:hasMolecule ?compound_chembl ;
		chembl:hasAssay ?assay_uri .
	?compound_chembl owl:sameAs ?chebi_compound .
	?assay_uri chembl:hasTarget ?target_uri .
	?target_uri a ?target_type .
	OPTIONAL { ?assay_uri chembl:organismName ?assay_organism }
	OPTIONAL { ?target_uri dcterms:title ?target_name }
	OPTIONAL { ?target_uri chembl:organismName ?target_organism }
	OPTIONAL { ?item chembl:standardType ?activity_type }
	OPTIONAL { ?item chembl:standardRelation ?activity_relation }
	OPTIONAL { ?item chembl:standardValue ?std_value .
	BIND(xsd:decimal(?std_value) as ?activity_value) }
	OPTIONAL { ?item chembl:standardUnits ?activity_unit }
	OPTIONAL { ?item chembl:pChembl ?pChembl }
}" ;
	] ;
	api:defaultViewer <#compoundTreePharmaViewer> .

<#compoundTreePharmaViewer> a api:Viewer ;
        api:name "compoundTreePharmaViewer" ;
	api:template 
"?item chembl:hasMolecule ?compound_chembl;
	chembl:standardType ?activity_type;
	chembl:standardRelation ?activity_relation;
	chembl:standardValue ?activity_value;
        chembl:hasQUDT ?qudt_uri;
	chembl:publishedType ?published_type ;
        chembl:publishedRelation ?published_relation ;
        chembl:publishedValue ?published_value ;
        chembl:publishedUnits ?published_unit ;
	chembl:hasAssay ?assay_uri ;
	chembl:pChembl ?pChembl ;
        chembl:activityComment ?act_comment ;
        chembl:hasDocument ?doi ;
        bibo:pmid ?pmid ;
	void:inDataset <http://www.ebi.ac.uk/chembl> .
?qudt_uri skos:prefLabel ?activity_unit .
?assay_uri chembl:hasTarget ?target_uri ;
        chembl:assayOrganismName ?assay_organism ;
	dcterms:description ?assay_description ;
	chembl:assayTestType ?assay_type ;
        void:inDataset <http://www.ebi.ac.uk/chembl> .
?target_uri dcterms:title ?target_name ;
	chembl:targetOrganismName ?target_organism ;
        chembl:hasTargetComponent ?protein ;
        a ?target_type ;
        void:inDataset <http://www.ebi.ac.uk/chembl> . 
?compound_chembl skos:exactMatch ?compound_cw .
?protein skos:exactMatch ?cw_target .
?cw_target skos:prefLabel ?protein_name ;
        void:inDataset <http://www.conceptwiki.org> .
?compound_cw skos:prefLabel ?compound_name ;
	void:inDataset <http://www.conceptwiki.org> .
?compound_chembl skos:exactMatch ?compound_ocrs .
?compound_ocrs ops:smiles ?smiles ;
	ops:inchi ?inchi ;
	ops:inchikey ?inchiKey;
        ops:ro5_violations ?num_ro5_violations ;
	ops:molweight ?molweight ;
	void:inDataset <http://ops.rsc.org> .
?compound_chembl void:inDataset <http://www.ebi.ac.uk/chembl> ." ;
	api:where 
"		GRAPH <http://www.ebi.ac.uk/chebi/inference>  {
        		?class rdfs:subClassOf ?ops_item
		}
		GRAPH <http://www.ebi.ac.uk/chebi> {
		        ?chebi_compound rdfs:subClassOf|rdfs:subClassOf/owl:someValuesFrom ?class .
		}
		GRAPH <http://ops.rsc.org> {
                        ?compound_ocrs skos:exactMatch ?compound_chembl .
                        ?compound_ocrs cheminf:CHEMINF_000018 ?smiles ;
                                cheminf:CHEMINF_000396 ?inchi ;
                                cheminf:CHEMINF_000399 ?inchiKey .
                        OPTIONAL { [] obo:IAO_0000136 ?compound_ocrs ;
                                a cheminf:CHEMINF_000367;
                                qudt:numericValue ?num_ro5_violations . }
			OPTIONAL { [] obo:IAO_0000136 ?compound_ocrs ;
					a cheminf:CHEMINF_000484 ;
					qudt:numericValue ?molweight . }
                }
                GRAPH <http://www.conceptwiki.org> {
                        ?compound_cw skos:exactMatch ?compound_ocrs ;
                                skos:prefLabel ?compound_name .
                }
		GRAPH <http://www.ebi.ac.uk/chembl> {
        		?item a chembl:Activity ;
				chembl:hasMolecule ?compound_chembl ;
                		chembl:hasAssay ?assay_uri .
			?compound_chembl owl:sameAs ?chebi_compound .
        		?assay_uri chembl:hasTarget ?target_uri .
		        ?target_uri a ?target_type .
        		OPTIONAL { ?assay_uri chembl:organismName ?assay_organism }
        		OPTIONAL { ?assay_uri dcterms:description ?assay_description }
		        OPTIONAL { ?assay_uri chembl:assayTestType ?assay_type }
        		OPTIONAL { ?target_uri dcterms:title ?target_name }
        		OPTIONAL { ?target_uri chembl:organismName ?target_organism }
		        OPTIONAL { ?target_uri chembl:hasTargetComponent ?protein .
		                GRAPH <http://www.conceptwiki.org> {
                		        ?cw_target skos:exactMatch ?protein ;
                                		skos:prefLabel ?protein_name
		                }
		        }
			OPTIONAL { ?item chembl:publishedType ?published_type }
			OPTIONAL { ?item chembl:publishedRelation ?published_relation }
			OPTIONAL { ?item chembl:publishedValue ?published_value }
			OPTIONAL { ?item chembl:publishedUnits ?published_unit }
        		OPTIONAL { ?item chembl:standardType ?activity_type }
        		OPTIONAL { ?item chembl:standardRelation ?activity_relation }
        		OPTIONAL { ?item chembl:standardValue ?std_value .
			BIND(xsd:decimal(?std_value) as ?activity_value) }
        		OPTIONAL { ?item chembl:standardUnits ?activity_unit }
        		OPTIONAL { ?item chembl:hasQUDT ?qudt_uri }
			OPTIONAL { ?item chembl:pChembl ?pChembl }
        		OPTIONAL { ?item chembl:activityComment ?act_comment }
        		OPTIONAL { ?item chembl:hasDocument ?document .
        			OPTIONAL { ?document owl:sameAs ?doi }
        			OPTIONAL { ?document bibo:pmid ?pmid }
        		}
        	}" .
