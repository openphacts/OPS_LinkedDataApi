<http://qudt.org/vocab/unit#MilliSecond> api:name "milli_second" .
<http://qudt.org/vocab/unit#Millimeter> api:name "millimeter" .
<http://qudt.org/vocab/unit#Micrometer> api:name "micrometer" .
<http://qudt.org/vocab/unit#Liter> api:name "liter" .

chembl:organismName api:name "assay_organism" ;
	api:label "assay_organism" ;
	api:value "A literal organism in ChEMBL." ;
	api:filterVariable "?assay_uri";
	a rdf:Property .
	
_:targetOrganism api:name "target_organism" ;
	api:label "target_organism" ;
	api:value "A literal target organism in ChEMBL." ;
	api:filterVariable "?chembl_uri";
	rdfs:subPropertyOf chembl:organismName ;
	a rdf:Property .

chembl:standardType api:name "activity_type" ;
	api:label "activity_type" ;
	api:filterVariable "?item";
	api:value "One of the activity types listed at /pharmacology/filters/activities" ;
	a rdf:Property .

chembl:hasQUDT api:name "activity_unit" ;
        api:label "activity_unit" ;
        api:value "The unit in which {activity_value} is given. See /pharmacology/filters/units/{activity_type} for allowed valued. For e.g. IC50: /pharmacology/filters/units/IC50" ; 
        api:filterVariable "?item";
        a rdf:Property .

chembl:standardValue api:name "activity_value" ;
	api:label "activity_value" ;
	api:value "Return activity values equal to this number." ;
	api:filterVariable "?item";
	a rdf:Property .

chembl:standardRelation api:name "relation" ;
        api:label "activity_relation" ;
        api:value ">, >=,= , < , <=" ;
        api:filterVariable "?item";
        a rdf:Property .

chembl:pChembl api:name "pChembl" ;
	api:label "pChembl" ;
	api:filterVariable "?item";
        api:value "The pChembl value of an activity" ;
	a rdf:Property .

<#input> api:name "uri" ;
	api:label "uri";
	api:value "A target URI. e.g.: http://www.conceptwiki.org/concept/00059958-a045-4581-9dc5-e5a08bb0c291" ;
	rdfs:range rdfs:Resource .

<#targetPharmacologyListEndpoint> a api:ListEndpoint ;
	api:uriTemplate "/target/pharmacology/pages?uri={uri}" ;
	api:variable <http://www.openphacts.org/units/MicrogramPerMilliliter> ;
	api:variable <http://www.openphacts.org/units/Millimolar> ;
	api:variable <http://www.openphacts.org/units/Nanomolar> ;
	api:variable <http://qudt.org/vocab/unit#Percent> ;
	api:variable <http://www.openphacts.org/units/MilligramPerMilliliter> ;
	api:variable <http://qudt.org/vocab/unit#MinuteTime> ;
	api:variable <http://qudt.org/vocab/unit#Gram> ;
	api:variable <http://www.openphacts.org/units/NanogramPerMilliliter> ;
	api:variable <http://qudt.org/vocab/unit#Hour> ;
	api:variable <http://www.openphacts.org/units/PicogramPerMilliliter> ;
	api:variable <http://www.openphacts.org/units/Micromolar> ;
	api:variable <http://www.openphacts.org/units/Molar> ;
	api:variable <http://www.openphacts.org/units/MilligramPerDeciliter> ;
	api:variable <http://qudt.org/vocab/unit#Day> ;
	api:variable <http://qudt.org/vocab/unit#SecondTime> ;
	api:variable <http://www.openphacts.org/units/Picomolar> ;
	api:variable <http://qudt.org/vocab/unit#Centimeter> ;
	api:variable <http://www.openphacts.org/units/GramPerLiter> ;
	api:variable <http://qudt.org/vocab/unit#InternationalUnitPerLiter> ;
	api:variable <http://qudt.org/vocab/unit#Kilogram> ;
	api:variable <http://qudt.org/vocab/unit#DegreeCelsius> ;
	api:variable <http://qudt.org/vocab/unit#MilliSecond> ;
	api:variable <http://qudt.org/vocab/unit#Millimeter> ;
	api:variable <http://qudt.org/vocab/unit#Micrometer> ;
	api:variable <http://qudt.org/vocab/unit#Liter> ;
	api:name "Target Pharmacology Paginated" ;
	api:description "A page of items corresponding to acitivity values in the LDC for a given target" ;
	api:resultsDescription "[] <span style=\"BACKGROUND-COLOR: #66FFCC\"> ops:input ?ops_item </span>.
?item <span style=\"BACKGROUND-COLOR: #66FFCC\"> chembl:forMolecule ?equiv_compound</span>;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\"> chembl:type ?std_type</span>;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\"> chembl:relation ?relation</span>;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\"> chembl:standardValue ?std_value</span>;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\"> chembl:standardUnits ?std_unit</span>;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\"> chembl-ops:normalisedValue ?activity_value </span>;
	<span style=\"BACKGROUND-COLOR: #66FFCC\"> chembl:onAssay ?equiv_assay </span>;
	<span style=\"BACKGROUND-COLOR: #FFCC99\"> bibo:pmid ?pmid </span>;
	<span style=\"BACKGROUND-COLOR: #FFCC99\"> chembl:citesAsDataSource ?doi </span>;
	<span style=\"BACKGROUND-COLOR: #66FFCC\"> void:inDataset &lt;http://www.ebi.ac.uk/chembl&gt; </span>.
?equiv_assay <span style=\"BACKGROUND-COLOR: #66FFCC\"> chembl:hasTarget ?equiv_target </span>;
	<span style=\"BACKGROUND-COLOR: #FFCC99\"> chembl:organism ?assay_organism </span>;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\"> chembl:hasDescription ?assay_description </span>;
        <span style=\"BACKGROUND-COLOR: #66FFCC\"> void:inDataset &lt;http://www.ebi.ac.uk/chembl&gt; </span>.
?equiv_target <span style=\"BACKGROUND-COLOR: #FFFFCC\"> dc:title ?target_name </span>;
	<span style=\"BACKGROUND-COLOR: #FFFFCC\"> ops:target_organism ?target_organism </span>;
	<span style=\"BACKGROUND-COLOR: #66FFCC\"> skos:exactMatch ?cw_uri </span>;
	<span style=\"BACKGROUND-COLOR: #66FFCC\"> void:inDataset &lt;http://www.ebi.ac.uk/chembl&gt; </span>.
?cw_uri <span style=\"BACKGROUND-COLOR: #66FFCC\"> skos:prefLabel ?target_name </span>;
        <span style=\"BACKGROUND-COLOR: #66FFCC\"> void:inDataset &lt;http://www.conceptwiki.org/&gt; </span>.
?equiv_compound <span style=\"BACKGROUND-COLOR: #FFFFCC\"> ?node1 ?molweight </span>;
	<span style=\"BACKGROUND-COLOR: #66FFCC\"> skos:exactMatch ?compound_cs </span>;
	<span style=\"BACKGROUND-COLOR: #66FFCC\"> skos:exactMatch ?compound_cw </span>;
	<span style=\"BACKGROUND-COLOR: #66FFCC\"> void:inDataset &lt;http://www.ebi.ac.uk/chembl&gt; </span>.
?compound_cs <span style=\"BACKGROUND-COLOR: #66FFCC\"> chemspider:smiles ?smiles </span>;
	<span style=\"BACKGROUND-COLOR: #66FFCC\"> chemspider:inchi ?inchi </span>;
	<span style=\"BACKGROUND-COLOR: #66FFCC\"> chemspider:inchikey ?inchi_key</span>;
        <span style=\"BACKGROUND-COLOR: #FFFFCC\"> ops:ro5_violations ?num_ro5_violations </span>;
	<span style=\"BACKGROUND-COLOR: #66FFCC\"> void:inDataset &lt;http://rdf.chemspider.com/&gt; </span>.
?compound_cw <span style=\"BACKGROUND-COLOR: #66FFCC\"> skos:prefLabel ?compound_name </span>;
	<span style=\"BACKGROUND-COLOR: #66FFCC\"> void:inDataset &lt;http://www.conceptwiki.org/&gt; </span>." ;

	api:exampleRequestPath "/target/pharmacology/pages?uri=http%3A%2F%2Fwww.conceptwiki.org%2Fconcept%2F00059958-a045-4581-9dc5-e5a08bb0c291";
	api:exampleRequestPath "/target/pharmacology/pages?uri=http%3A%2F%2Fwww.conceptwiki.org%2Fconcept%2F208b03ad-e809-4d98-b83b-69ff85880897&activity_type=Potency&activity_unit=nanomolar&maxEx-activity_value=10&minEx-activity_value=1&assay_organism=Mus%20musculus";
	api:selector [
		api:where 
"GRAPH <http://www.ebi.ac.uk/chembl> {
        ?assay_uri chembl:hasTarget ?chembl_uri .
        ?item a chembl:Activity ;
		chembl:hasAssay ?assay_uri ;
                chembl:hasMolecule ?compound_chembl .
	OPTIONAL { ?chembl_uri dcterms:title ?target_name_chembl }
        OPTIONAL { ?chembl_uri chembl:assayOrganismName ?target_organism }
        OPTIONAL { ?assay_uri chembl:targetOrganismName ?assay_organism }
        OPTIONAL { ?assay_uri dcterms:description ?assay_description }
	OPTIONAL { ?item chembl:publishedType ?published_type }
        OPTIONAL { ?item chembl:publishedRelation ?published_relation }
        OPTIONAL { ?item chembl:publishedValue ?published_value }
        OPTIONAL { ?item chembl:publishedUnits ?published_unit }
        OPTIONAL { ?item chembl:standardType ?standard_type }
	OPTIONAL { ?item chembl:standardRelation ?standard_relation }
	OPTIONAL { ?item chembl:standardValue ?standard_value }
	OPTIONAL { ?item chembl:standardUnits ?standard_unit }
	OPTIONAL { ?item chembl:hasQUDT ?qudt_uri }
	OPTIONAL { ?item chembl:pChembl ?pChembl }
        OPTIONAL { ?item chembl:hasDocument ?doc_uri .
		OPTIONAL { ?doc_uri owl:sameAs ?doi }
		OPTIONAL { ?doc_uri bibo:pmid ?pmid }
	}	
        OPTIONAL { ?compound_chembl sio:SIO_000008 ?full_mwt .
              		?full_mwt a sio:CHEMINF_000198 ;
 	      			sio:SIO_000300 ?molweight. }
}
GRAPH <http://www.chemspider.com> {
        ?compound_cs skos:exactMatch ?compound_chembl .
        ?compound_cs chemspider:inchi ?inchi;
                chemspider:inchikey ?inchi_key;
                chemspider:smiles ?smiles .
        OPTIONAL { [] obo:IAO_0000136 ?compound_cs ;
                a cheminf:CHEMINF_000367;
                qudt:numericValue ?num_ro5_violations . }

}
GRAPH <http://www.conceptwiki.org> {
        ?cw_uri skos:prefLabel ?target_name .
        ?compound_cw skos:exactMatch ?compound_cs ;
                skos:prefLabel ?compound_name .
}" ;
	] ;
	api:defaultViewer <#targetPharmacologyListViewer> .

<#targetPharmacologyListViewer> a api:Viewer ;
        api:name "targetPharmacologyListViewer" ;
	api:template 
"[] ops:input ?ops_item .
?item chembl:hasMolecule ?compound_chembl;
	chembl:publishedType ?published_type ;
        chembl:publishedRelation ?published_relation ;
	chembl:publishedValue ?published_value ;
	chembl:publishedUnits ?published_unit ;
	chembl:standardType ?standard_type ;
	chembl:standardRelation ?standard_relation ;
	chembl:standardValue ?standard_value ;
	chembl:standardUnits ?standard_unit ;
	chembl:hasQUDT ?qudt_uri;
	chembl:hasAssay ?assay_uri ;
	chembl:pChembl ?pChembl ;
	bibo:pmid ?pmid ;
	chembl:hasDocument ?doi ;
	void:inDataset <http://www.ebi.ac.uk/chembl> .
?assay_uri chembl:assayOrganismName ?assay_organism ;
	chembl:hasTarget ?chembl_uri ; 
	dcterms:description ?assay_description ;
        void:inDataset <http://www.ebi.ac.uk/chembl> .
?chembl_uri dcterms:title ?target_name_chembl ;
	chembl:targetOrganismName ?target_organism ;
	skos:exactMatch ?cw_uri ;
	void:inDataset <http://www.ebi.ac.uk/chembl> .
?chembl_uri skos:exactMatch ?cw_uri . 
?cw_uri skos:prefLabel ?target_name ;
        void:inDataset <http://www.conceptwiki.org/> .
?compound_chembl ?full_mwt ?molweight ;
	skos:exactMatch ?compound_cs ; 
	skos:exactMatch ?compound_cw . 
?compound_cs chemspider:smiles ?smiles ;
	chemspider:inchi ?inchi ;
	chemspider:inchikey ?inchi_key;
        ops:ro5_violations ?num_ro5_violations ;
	void:inDataset <http://rdf.chemspider.com/> .
?compound_cw skos:prefLabel ?compound_name ;
	void:inDataset <http://www.conceptwiki.org/> .
" ; 
	api:where 
"GRAPH <http://www.ebi.ac.uk/chembl> {
        ?assay_uri chembl:hasTarget ?chembl_uri .
        ?item a chembl:Activity ;
	      chembl:hasAssay ?assay_uri ;
              chembl:hasMolecule ?compound_chembl .
	OPTIONAL { ?chembl_uri dcterms:title ?target_name_chembl }
        OPTIONAL { ?chembl_uri chembl:organismName ?target_organism }
        OPTIONAL { ?assay_uri chembl:organismName ?assay_organism }
        OPTIONAL { ?assay_uri dcterms:description ?assay_description }
	OPTIONAL { ?item chembl:publishedType ?published_type }
        OPTIONAL { ?item chembl:publishedRelation ?published_relation }
        OPTIONAL { ?item chembl:publishedValue ?published_value }
        OPTIONAL { ?item chembl:publishedUnits ?published_unit }
        OPTIONAL { ?item chembl:standardType ?standard_type }
	OPTIONAL { ?item chembl:standardRelation ?standard_relation }
	OPTIONAL { ?item chembl:standardValue ?standard_value }
	OPTIONAL { ?item chembl:standardUnits ?standard_unit }
	OPTIONAL { ?item chembl:hasQUDT ?qudt_uri }
	OPTIONAL { ?item chembl:pChembl ?pChembl }
        OPTIONAL { ?item chembl:hasDocument ?doc_uri .
		OPTIONAL { ?doc_uri owl:sameAs ?doi }
		OPTIONAL { ?doc_uri bibo:pmid ?pmid }
	}	
        OPTIONAL { ?compound_chembl sio:SIO_000008 ?full_mwt .
              		?full_mwt a sio:CHEMINF_000198 ;
 	      			sio:SIO_000300 ?molweight. }
}
GRAPH <http://www.chemspider.com> {
        ?compound_cs skos:exactMatch ?compound_chembl .
        ?compound_cs chemspider:inchi ?inchi;
                chemspider:inchikey ?inchi_key;
                chemspider:smiles ?smiles .
        OPTIONAL { [] obo:IAO_0000136 ?compound_cs ;
                a cheminf:CHEMINF_000367;
                qudt:numericValue ?num_ro5_violations . }

}
GRAPH <http://www.conceptwiki.org> {
        ?cw_uri skos:prefLabel ?target_name .
        ?compound_cw skos:exactMatch ?compound_cs ;
                skos:prefLabel ?compound_name .
}" .
